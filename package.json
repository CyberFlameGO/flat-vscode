{
  "name": "flat",
  "displayName": "flat",
  "publisher": "GitHubOCTO",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/title": [
        {
          "command": "flat.saveAndCommit",
          "group": "navigation",
          "when": "resourceScheme == flat"
        }
      ]
    },
    "commands": [
      {
        "command": "flat.createAction",
        "title": "Create Action"
      },
      {
        "command": "flat.saveAndCommit",
        "title": "Save and Commit Action"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "repositories",
          "name": "Repositories",
          "contextualTitle": "Repositories"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "dev": "parcel src/webview/index.js --dist-dir ./media",
    "build": "parcel build <your entry file>"
  },
  "devDependencies": {
    "@parcel/optimizer-cssnano": "2.0.0-nightly.607",
    "@parcel/packager-css": "2.0.0-nightly.607",
    "@parcel/transformer-css": "2.0.0-nightly.607",
    "@parcel/transformer-postcss": "2.0.0-nightly.607",
    "@tailwindcss/forms": "^0.2.1",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "parcel": "^2.0.0-nightly.605",
    "postcss": "^8.2.6",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.1.0",
    "@octokit/types": "^6.8.3",
    "connection-string": "^4.3.2",
    "cronstrue": "^1.109.0",
    "formik": "^2.2.6",
    "pg": "^8.5.1",
    "query-string": "^6.14.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.9.8",
    "react-use": "^17.1.1",
    "typeorm": "^0.2.31",
    "wretch": "^1.7.4",
    "yup": "^0.32.9"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
